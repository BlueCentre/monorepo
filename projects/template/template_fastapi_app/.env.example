# Application settings
PROJECT_NAME=Template FastAPI App
PORT=8000
RELOAD=true
API_V1_STR=/api/v1

# Security
SECRET_KEY=your-secret-key-here
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Database
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=app
SQLALCHEMY_DATABASE_URI=postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_SERVER}/${POSTGRES_DB}

# First superuser
FIRST_SUPERUSER_EMAIL=admin@example.com
FIRST_SUPERUSER_PASSWORD=admin

# CORS
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8080"]

# Google Cloud
GCP_PROJECT_ID=your-gcp-project-id

# PubSub
PUBSUB_TOPIC_EXAMPLE=example
PUBSUB_SUBSCRIPTION_EXAMPLE=example-subscription

# OpenTelemetry
ENABLE_TELEMETRY=true
OTLP_EXPORTER_ENDPOINT=http://otel-collector:4317
OTLP_SERVICE_NAME=template-fastapi-app

# Environment
ENVIRONMENT=development 